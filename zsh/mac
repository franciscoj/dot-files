# -*- mode: shell-script; -*-

if [[ -z $TMUX ]]; then
    export PATH=$NEW_PATH
fi

# Load autojump from homebrew
[[ -s $(brew --prefix)/etc/profile.d/autojump.sh ]] && . $(brew --prefix)/etc/profile.d/autojump.sh

# Load nvm
export NVM_DIR=~/.nvm
[[ -s $(brew --prefix nvm)/nvm.sh ]] && source $(brew --prefix nvm)/nvm.sh
